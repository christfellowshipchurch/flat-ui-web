{
  "name": "flat-ui-web",
  "version": "1.0.0",
  "description": "Flat UI package for Christ Fellowship web projects",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "lint": "eslint ./src --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christfellowshipchurch/flat-ui-web.git"
  },
  "author": "Christ Fellowship Church <developer@cftoday.org>",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/christfellowshipchurch/flat-ui-web/issues"
  },
  "peerDependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "html-to-react": "^1.3.4",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "popper.js": "^1.14.7",
    "prop-types": "^15.7.2",
    "react-image": "^2.0.0",
    "react-scripts": "2.1.5",
    "reactstrap": "^7.1.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "reporters": [
      "<rootDir>/../../scripts/fingers-crossed-reporter.js"
    ]
  },
  "homepage": "https://github.com/christfellowshipchurch/flat-ui-web#readme",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}