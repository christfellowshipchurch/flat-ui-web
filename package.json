{
  "name": "@christfellowshipchurch/flat-ui-web",
  "version": "0.0.4",
  "description": "Flat UI package for Christ Fellowship web projects",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "lint": "eslint ./src --ext .js",
    "babel-version": "babel --version"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christfellowshipchurch/flat-ui-web.git"
  },
  "author": "Christ Fellowship Church <developer@cftoday.org>",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/christfellowshipchurch/flat-ui-web/issues"
  },
  "devDependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "babel-cli": "^6.6.5",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.5.0"
  },
  "peerDependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "html-to-react": "^1.3.4",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "popper.js": "^1.14.7",
    "prop-types": "^15.7.2",
    "react-image": "^2.0.0",
    "reactstrap": "^7.1.0"
  },
  "babel": {
    "presets": [
      "react"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/christfellowshipchurch/flat-ui-web#readme",
  "publishConfig": {
    "access": "public"
  },
